# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.ts]
quote_type = double

[*.json]
quote_type = double

[*.yml]
quote_type = double

[*.yaml]


[*.md]
trim_trailing_whitespace = true
